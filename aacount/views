        addresse = form.cleaned_data['sujet']
        ville = form.cleaned_data['sujet']
        code_postale = form.cleaned_data['sujet']

        sexe = form.cleaned_data['sujet']
        date = form.cleaned_data['sujet']
        statut = form.cleaned_data['sujet']
        print("kikette")

        
        envoi = True
    return render(request, 'pages/inscription.html',locals())





